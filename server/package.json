{
  "name": "featureforge-server",
  "version": "1.0.0",
  "description": "Backend API for FeatureForge - Feature Prioritization Tool",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "dev:db-required": "nodemon src/index.js --require-db",
    "test": "NODE_ENV=test jest",
    "test:setup": "node src/scripts/setupTestDb.js",
    "test:with-setup": "npm run test:setup && npm test",
    "test:email": "node src/scripts/testEmailService.js",
    "init-db": "node src/scripts/initDb.js",
    "check-db": "node src/scripts/checkDbConnection.js",
    "clear-db": "node src/scripts/clearDb.js",
    "seed": "node src/scripts/seedDb.js",
    "db:migrate": "sequelize-cli db:migrate",
    "db:migrate:undo": "sequelize-cli db:migrate:undo",
    "db:migrate:undo:all": "sequelize-cli db:migrate:undo:all",
    "db:seed:all": "sequelize-cli db:seed:all",
    "db:seed:undo:all": "sequelize-cli db:seed:undo:all"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.54.0",
    "@react-email/components": "^0.1.1",
    "@react-email/render": "^1.1.3",
    "bcryptjs": "^2.4.3",
    "bull": "^4.16.5",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.0",
    "firebase-admin": "^11.11.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.3",
    "morgan": "^1.10.0",
    "nodemailer": "^6.10.0",
    "openai": "^5.6.0",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "resend": "^4.6.0",
    "sequelize": "^6.35.1",
    "swagger-ui-express": "^5.0.1",
    "validator": "^13.11.0",
    "winston": "^3.17.0",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "sequelize-cli": "^6.6.2",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
